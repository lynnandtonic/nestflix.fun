// rubik-300 - latin
@font-face
  font-family: 'Rubik'
  font-style: normal
  font-weight: 300
  src: local(''),
       url('/assets/fonts/rubik-v11-latin-300.woff2') format('woff2'), // Chrome 26+, Opera 23+, Firefox 39+
       url('/assets/fonts/rubik-v11-latin-300.woff') format('woff') // Chrome 6+, Firefox 3.6+, IE 9+, Safari 5.1+

// rubik-500 - latin
@font-face
  font-family: 'Rubik'
  font-style: normal
  font-weight: 500
  src: local(''),
       url('/assets/fonts/rubik-v11-latin-500.woff2') format('woff2'), // Chrome 26+, Opera 23+, Firefox 39+
       url('/assets/fonts/rubik-v11-latin-500.woff') format('woff') // Chrome 6+, Firefox 3.6+, IE 9+, Safari 5.1+

// rubik-300italic - latin
@font-face
  font-family: 'Rubik'
  font-style: italic
  font-weight: 300
  src: local(''),
       url('/assets/fonts/rubik-v11-latin-300italic.woff2') format('woff2'), // Chrome 26+, Opera 23+, Firefox 39+
